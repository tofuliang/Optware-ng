+
+# type: TP
+#. type: TP
+#: client.conf.man.in:77
+#, no-wrap
+msgid "B<User >I<name>"
+msgstr "B<User> I<nom>"
+
+# type: Plain text
+#. type: Plain text
+#: client.conf.man.in:80
