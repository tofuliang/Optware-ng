+# type: TH
+#. type: TH
+#: cupsaccept.man:13
+#, no-wrap
+msgid "cupsaccept"
+msgstr "cupsaccept"
+
+# type: TH
+#. type: TH
+#: cupsaccept.man:13 cups-snmp.conf.man:13 cups-snmp.man.in:12
+#, no-wrap
+msgid "10 June 2014"
